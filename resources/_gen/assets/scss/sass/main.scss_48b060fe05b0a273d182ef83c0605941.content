@import "https://fonts.googleapis.com/css2?family=Questrial&display=swap";@import "https://fonts.googleapis.com/css2?family=Roboto+Mono&display=swap";body{font-family:roboto mono,monospace;font-size:16px;line-height:1.5;font-weight:300;color:#ffffffde;background-color:#121212;-webkit-text-size-adjust:100%}a{color:#ffffffde;text-decoration:none}a:visited{color:#ffffffde}a:hover{color:#ffffffde}article{margin-top:1em}article img{margin-bottom:2em}@media screen and (min-width:0px) and (max-width:770px){article img{margin-bottom:1em}}article img:last-of-type{margin-bottom:0}.flex{display:flex;justify-content:space-between}.flex time{margin-top:.55em;color:#fff9}@media screen and (min-width:0px) and (max-width:992px){.flex time{margin-top:.3em}}@media screen and (min-width:0px) and (max-width:500px){.flex time{display:none}}@media screen and (min-width:0px) and (max-width:500px){.container{padding:0}}@media screen and (min-width:0px) and (max-width:500px){.title{font-size:16pt;padding-left:.6em}}header{font-family:questrial,sans-serif;text-align:center;padding-top:3em;padding-bottom:3em}@media screen and (min-width:0px) and (max-width:770px){header{padding-top:1em;padding-bottom:1em}}header h1{font-size:56pt}@media screen and (min-width:0px) and (max-width:770px){header h1{font-size:35pt;margin-bottom:0}}@media screen and (min-width:0px) and (max-width:500px){header h1{font-size:25pt;margin-bottom:0}}header ul{list-style-type:none;margin:0;padding:0}header li{display:inline;margin-left:5px}header a:link,header a:visited{text-decoration:inherit;color:inherit}header nav a:link,header nav a:visited{color:#fff9}header nav a:hover{color:#fff9}img{width:100%}.circle-logo{width:100px;border-radius:50%;margin-right:10px}@media screen and (min-width:0px) and (max-width:770px){.circle-logo{width:50px}}input,textarea{width:100%;padding:12px 20px;margin:8px 0;display:inline-block;background-color:#222;color:#ffffffde;border:1px solid #424242;border-radius:4px;box-sizing:border-box}i